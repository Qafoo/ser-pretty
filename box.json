{
    "alias": "ser-pretty.phar",
    "chmod": "0755",
    "directories": ["src"],
    "files": [
        "README.md"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests"],
            "in": ["vendor", "src"]
        }
    ],
    "git-version": "package_version",
    "main": "src/bin/ser-pretty",
    "output": "ser-pretty.phar",
    "stub": true
}
